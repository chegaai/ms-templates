{
  "name": "ms-templates",
  "version": "0.0.1",
  "description": "Templates microservice",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build:clean",
    "clean": "rm -rf ./dist",
    "test": "exit 0",
    "coverage": "exit 0",
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "build:debug": "npm run clean && npm run build -- --sourceMap",
    "build:clean": "npm run clean && npm run build",
    "build:watch": "tsc -w",
    "start": "node dist/index.js",
    "start:debug": "tsc && concurrently -s all \"tsc -w\" \"DEBUG=expresso:* nodemon dist/index.js\""
  },
  "license": "GPL-3.0",
  "dependencies": {
    "@azure/storage-blob": "^10.5.0",
    "@expresso/app": "^1.1.1",
    "@expresso/errors": "^1.3.0",
    "@expresso/server": "^1.1.3",
    "@expresso/validator": "^1.0.5",
    "@nindoo/mongodb-data-layer": "^2.0.2",
    "axios": "^0.19.0",
    "ejs": "^2.7.1",
    "express-rescue": "^1.1.22",
    "i": "^0.3.6",
    "npm": "^6.12.0",
    "puppeteer-core": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "sugar-env": "^1.5.7",
    "tsyringe": "^3.4.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/ejs": "^2.6.3",
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.8",
    "@types/puppeteer-core": "^1.9.0",
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.6.3"
  }
}
